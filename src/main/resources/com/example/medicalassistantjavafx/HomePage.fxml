<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="555.0" prefWidth="600.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.medicalassistantjavafx.HomePageController">
   <children>
      <StackPane />
      <Label alignment="TOP_CENTER" layoutX="116.0" layoutY="14.0" styleClass="label-title" text="Medical Assistant">
         <font>
            <Font name="Candara Light" size="48.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" layoutX="200.0" layoutY="138.0" spacing="20">
         <Button fx:id="profile" mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0" styleClass="button" text="Signup" />
         <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0" text="Login" />
         <Button fx:id="calendar" mnemonicParsing="false" onAction="#handleCalendarReviewButtonClick" prefHeight="40.0" prefWidth="200.0" styleClass="button" text="Calendar" />
         <Button fx:id="prescription" mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0" styleClass="button" text="E-Prescription" />
   
            <Button fx:id="doctorreview" mnemonicParsing="false" onAction="#handleDoctorReviewButtonClick" prefHeight="40.0" prefWidth="200.0" styleClass="button" text="Doctor Review" />
   
         <Button fx:id="speaktodoctor" mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0" styleClass="button" text="Speak to a Doctor" />
      </VBox>
   </children>
</AnchorPane>
