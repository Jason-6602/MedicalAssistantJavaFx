<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="341.0" prefWidth="389.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="com.example.medicalassistantjavafx.SignUpController">

    <Label layoutX="174.0" layoutY="27.0" text="Sign Up" />

    <Label layoutX="40" layoutY="60" text="First Name:" />
    <TextField fx:id="firstNameField" layoutX="130" layoutY="60" />

    <Label layoutX="40" layoutY="100" text="Last Name:" />
    <TextField fx:id="lastNameField" layoutX="130" layoutY="100" />

    <Label layoutX="40" layoutY="140" text="Email:" />
    <TextField fx:id="emailField" layoutX="130" layoutY="140" />

    <Label layoutX="40" layoutY="180" text="Password:" />
    <PasswordField fx:id="passwordField" layoutX="130" layoutY="180" />

    <Label layoutX="20" layoutY="220" text="Confirm Password:" />
    <PasswordField fx:id="confirmPasswordField" layoutX="130" layoutY="220" />

    <Button layoutX="166.0" layoutY="259.0" onAction="#handleSignup" text="Sign Up" />

    <Label fx:id="infoLabel" layoutX="40" layoutY="300" />

</AnchorPane>